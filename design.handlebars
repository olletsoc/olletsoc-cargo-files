<script src="https://assets.calendly.com/assets/external/widget.js" type="text/javascript"></script>
<script type="text/javascript">Calendly.initBadgeWidget({url: 'https://calendly.com/olletsoc', text: 'Schedule time with me', color: '#00a2ff', branding: true});</script>
