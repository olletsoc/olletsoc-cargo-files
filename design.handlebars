		{{{ site.custom_html }}}

		{{! The global loading indicator }}
		<div class="loading" data-loading="page">
			{{>loading_animation}}
		</div>
		<div class="site_header">
			{{#if site.header_url}}
			<div class="header_image">
				<div>
					{{#if_option "header_link_to_index"}}
					{{#if_any route.search route.is_set}}
					<h1 class="overlay-white"><a href="{{ site.direct_link }}">
						{{else}}
						<a href="{{ site.direct_link }}" rel="show_index">
							{{/if_any}}
							{{/if_option}}
							<!-- <img src="{{ site.header_url }}" {{#if display_options.header_2x}} height="{{ site.header_height_2x }}" width="{{ site.header_width_2x }}" {{else}} height="{{ site.header_height }}" width="{{ site.header_width }}" {{/if}} />-->
							<img src="{{ site.header_url }}" {{#if display_options.header_2x}} {{/if}} />
							{{#if_option "header_link_to_index"}}
						</a></h1>
						{{/if_option}}
						</div>
				</div>
				<div class="tagline">
					<h2>The creative showspace of Anthony Costello</h2>
				</div>
				{{/if}}
			{{! Navigation }}
			<div class="navigation">
					<span id="autopagination_wrapper" data-view="Navigation" data-set="Navigation">

					{{#navigation}}
						<div id="menu_{{ id }}" class="{{ type }}_link" data-type="{{ type }}" data-id="{{ id }}">
							<a id="p{{ id }}" {{#if link_url }}target="{{ link_target }}" href="{{ link_url }}"{{else}} href="{{ url }}" {{#unless_all is_set route.search}}rel="history"{{/unless_all}}{{/if}}>
								{{ title_no_html }}
							</a>
						</div>
					{{/navigation}}
					</span>
<!-- 						{{#if_option "header_link_to_index"}}
							{{#if_any route.search route.is_set}}
								<h1><a href="{{ site.direct_link }}">
							{{else}}
								<a href="{{ site.direct_link }}" rel="show_index">
							{{/if_any}}
						{{/if_option}}
							<img src="{{ site.header_url }}" {{#if display_options.header_2x}} height="{{ site.header_height_2x }}" width="{{ site.header_width_2x }}" {{else}} height="{{ site.header_height }}" width="{{ site.header_width }}" {{/if}} />
						{{#if_option "header_link_to_index"}}
							</a></h1>
						{{/if_option}} -->
						<div class="about">
							<div style="max-width: 480px;">
							<span></span>
							</div>
						</div>

			<!--<div class="header_text">
				<a href="{{ site.direct_link }}" {{#if_any route.search route.is_set}}{{else}}rel="show_index"{{/if_any}}>
					{{{ site.header_text }}}
				</a>
			</div> -->
<!--<span id="autopagination_wrapper" data-view="Navigation" data-set="Navigation">
				{{#navigation}}
					<div id="menu_{{ id }}" class="{{ type }}_link" data-type="{{ type }}" data-id="{{ id }}">
						<a id="p{{ id }}" {{#if link_url }}target="{{ link_target }}" href="{{ link_url }}"{{else}} href="{{ url }}" {{#unless_all is_set route.search}}rel="history"{{/unless_all}}{{/if}}>
							{{ title_no_html }}
						</a>
					</div>
				{{/navigation}}
				</span>
 -->
				{{#if_option "following_link_in_nav"}}
				<div class="following" data-type="following">
					<a href="{{ site.following_url }}">Following</a> ({{ site.following_count }})
				</div>
				{{/if_option}}

			</div>

			{{#if site.filter}}
			<div class="filter">Filter: {{ site.filter }}</b> <a class="view_all" href="{{ site.direct_link }}">view all</a>
			</div>
			{{/if}}

		</div>

{{#route index "/"}}
		<div class="container">

			<div id="project" class="project grid clear" data-view="ProjectDetail" data-set="Project">

				<div class="entry" data-id="{{ project.id }}" data-resize-parent>

					<div class="project_nav" data-type="{{ project.type }}">
						{{#if_option "next_link_in_project"}}
							<a href="#" class="previous" rel="prev_project">&#11013;</a>
						{{/if_option}}

						{{#if_option "index_link_in_project"}}
							<a href="#" class="index" rel="show_index">
								<span>&nbsp;</span>
								<span>&nbsp;</span>
								<span>&nbsp;</span>
								<span>&nbsp;</span>
							</a>
						{{/if_option}}

						{{#if_option "next_link_in_project"}}
							<a href="#" class="next" rel="next_project">&#11013;</a>
						{{/if_option}}
					</div>

					<div class="project_header">

						{{#if_option "title_in_project"}}
							{{#if project.show_title}}
								<div class="project_title">
									{{{ project.title }}}
								</div>
							{{/if}}
						{{/if_option}}

					</div>

					<!-- <div class="project_content" data-elementresizer>
						{{print "project.content"}}
					</div> -->
					<div class="project_content">
						{{print "project.content"}}
					</div>

					<div class="project_footer">

						{{#if project.date}}
							<span class="date">
								{{ project.date }}
							</span>
						{{/if}}

						{{#if project.tags}}
							<span class="tags">
								Filed under
								{{#project.tags}}<span><a href="{{ filter_link }}">{{ tag }}</a></span>{{/project.tags}}
							</span>
						{{/if}}

						{{#if_option "view_count_in_project"}}
							<span class="views">({{ project.views }} views)</span>
						{{/if_option}}

						{{#if site.is_editor}}
							<span class="edit_site">
								<a href="{{ site.direct_link }}/admin/{{ project.id }}" target="_top">Edit</a>
							</span>
						{{/if}}

					</div>

					<directlink link="{{ project.direct_link }}" id="dl_{{ project.id }}"></directlink>

				</div>

			</div>

		</div>

		<div id="index" class="thumbnails clear white" data-columnize=".thumbnail" data-columnize-equalize="false" data-columnize-fluid="{{#if_option "fluid_columns"}}true{{/if_option}}" data-columnize-width="{{ display_options.thumb_size.w }}" data-view="Thumbnail" data-set="Projects">
			{{#projects}}
<!-- 				<figure class="effect-zoe">
					<img src="img/14.jpg" alt="img14"/>
					<figcaption>
						<h2>Creative <span>Zoe</span></h2>
						<span class="icon-heart"></span>
						<span class="icon-eye"></span>
						<span class="icon-paper-clip"></span>
						<p>Zoe never had the patience of her sisters. She deliberately punched the bear in his face.</p>
						<a href="#">View more</a>
					</figcaption>
				</figure> -->
				<div class="thumbnail" id="item_{{ id }}" name="{{ id }}" data-setid="{{ set_id }}">
<!-- 						<a href="{{ url }}" data-id="{{ id }}" rel="history">
							{{#if_option "title_in_thumb"}}
								<div class="thumb_title">
									<div class="thumb_title_inner">{{{ title }}}</div>
								</div>
							{{/if_option}}
						</a> -->
						<div class="loading" data-loading="{{ id }}">
							{{>loading_animation}}
						</div>
						<a href="{{ url }}" data-id="{{ id }}" rel="history">
								<div class="thumb_image" name="">
									<div>
										<img src="{{print "thumb_url"}}" width="100%" height="{{ thumb_height }}" {{#if thumb_url_2x}} data-hi-res="{{ thumb_url_2x }}" {{/if}}>
										<a href="{{ url }}" data-id="{{ id }}" rel="history">
											{{#if_option "title_in_thumb"}}
												<div class="thumb_title">
													<div class="thumb_title_inner">{{ title }}</div>
												</div>
											{{/if_option}}
										</a>
									</div>

								</div>
						</a>
					{{#if_option "excerpt_in_thumb"}}
						<div class="thumb_excerpt">
							<span>{{ excerpt }}</span>
						</div>
					{{/if_option}}

					{{#if_option "tags_in_thumb"}}
						<div class="thumb_tags">
							{{#tags}}
							<span class="thumb_tag"><a href="{{ filter_link }}">{{ tag }}</a></span>
							{{/tags}}
						</div>
					{{/if_option}}
				</div>
			{{/projects}}

		</div>
		<div class="show_index" style="display: none;">
			<a href="{{ site.direct_link }}" {{#if_any route.search route.is_set}}{{else}}rel="show_index"{{/if_any}}>{{#if_any route.search route.is_set}}View All{{else}}Index{{/if_any}}</a>
		</div>

		<div id="autopaginate_loader" class="moreload" data-view="Autopaginate" data-preload_distance="500">
			{{>loading_animation}}
		</div>

{{/route}}


{{#route search "/search"}}

		<div id="search" class="search" data-view="Search">
			<div action="#" class="search_form_results" id="search_form_results">
				<div class="search_header">Search results for </div>
				<input type="text" value="{{ search.term }}" id="results_search_term">
			</div>
			{{#search}}
				{{#projects}}
				<div class="result">
					<div class="search_thumb">
						<a href="{{ direct_link }}">
							<img border="0" align="left" src="{{ thumb_url }}">
						</a>
					</div>
					<div class="search_text">
						<div class="search_title">
							<a href="{{ direct_link }}">{{ title }}</a>
						</div>
						<div class="search_body">
							{{ excerpt }}
						</div>
						<div class="search_tags">
							{{#tags}}
								<span class="thumb_tag"><a href="{{ filter_link }}">{{ tag }}</a></span>
							{{/tags}}
						</div>
					</div>
				</div>
				{{/projects}}
			{{/search}}
		</div>

{{/route}}


	</body>
</html>
